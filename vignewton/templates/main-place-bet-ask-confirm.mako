<div class="main-placebet-view">
  <div class="placebet-header">
    <p>${gameline}</p>
    <p>${betline}</p>
  </div>
  <div>Betting $${amount}?</div>
  <div class="action-button" id="confirm-bet-button">Confirm</div>
  <div class="action-button" id="cancel-bet-button">Cancel</div>
  <input type="hidden" id="bet-amount" value="${amount}">
  <input type="hidden" id="confirm-url" value="${confirm_url}">
  <input type="hidden" id="cancel-url" value="${cancel_url}">
</div>

